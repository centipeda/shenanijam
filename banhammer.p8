pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- shenanijam 2017 game
-- "slinging the banhammer"
level = 0
turntime = 50 -- in ticks
new = true
ents = {}
cur = {x = 0,
							y = 0}
curstate = 0
function _init()

end

function check_input()

function collect_ents()
	-- loops through the entire map
	-- and checks whether each sprite
	-- is a troll/infected/normal
	for x = 0, 16, 2 do
		for y = 0, 16, 2	do
			local state = mget(x,y)
			if state == 40 or
						state == 42 or
						state == 44 then
				add(ents,{x=x,
														y=y})
			end
		end
	end
	print(ents)				
end

function update_ents()
	if turntime == 0 then
		infect()
		turntime = 150
	end
end

function infect()
	-- checks the adjacent squares
	-- of each collected entity.
	-- if its sprite # is lower, then
	-- it's able to be infected
	-- and we set the sprite number
	-- one higher to infect it
	for e in all(ents) do
		-- sprite number of ent
		local current = mget(e.x,e.y)
		local l = mget(e.x-2,e.y)
		local r = mget(e.x+2,e.y)
		local u = mget(e.x,e.y-2)
		local d = mget(e.x,e.y+2)
		if current == 44 then
			local skip = true
			if l == 40 and rnd(10) > 5 then
				mset(e.x-2,e.y,42)
				mset(e.x-1,e.y,43)
				mset(e.x-2,e.y+1,58)
				mset(e.x-1,e.y+1,59)
			end
			if r == 40 and rnd(10) > 5 then
				mset(e.x+2,e.y,42)
				mset(e.x+1,e.y,43)
				mset(e.x+2,e.y+1,58)
				mset(e.x+1,e.y+1,59)
			end
			if u == 40 and rnd(10) > 5 then
				mset(e.x,e.y-2,42)
				mset(e.x+1,e.y-2,43)
				mset(e.x,e.y-1,58)
				mset(e.x+1,e.y-1,59)
			end
			if d == 40 and rnd(10) > 5 then
				mset(e.x,e.y+2,42)
				mset(e.x+1,e.y+2,43)
				mset(e.x,e.y+1,58)
				mset(e.x+1,e.y+2,59)
			end
		end
		if current == 42 and rnd(20) > 10 and
			not skip then 
				mset(e.x,e.y,44)
				mset(e.x+1,e.y,45)
				mset(e.x,e.y+1,60)
				mset(e.x+1,e.y+1,61)
				skip = false
		end
	end
end

function _update()
	turntime -= 1
	check_input()
	collect_ents()
	update_ents()
	last = ents
	ents = {}
end
		
function printents(ent)
	local y = 7
	for a,b in pairs(ent) do
		print(a.." "..b,0,y,8)						
		y += 7
	end
end		
				
function draw_board()
	map(level * 16,0,0,0,16,16)
end

function draw_cursor()
	
end
function _draw()
	rectfill(0,0,128,128,15)		
	draw_board()
	draw_cursor()
	print(turntime,0,0,7)
	foreach(last,printents)
	last = {}
	print(mget(4,6),10,0,7)
end
